package gov.nih.nci.ncia.markup;

import gov.nih.nci.ncia.dto.MarkupDTO;

public interface MarkupManager {

	/** Processes requests for both HTTP <code>GET</code> and <code>POST</code> methods.
	 */

	public abstract String getMarkups(MarkupDTO dto);

	public abstract void saveMarkup(MarkupDTO dto);

	public abstract void updateMarkup(MarkupDTO dto);

	public abstract void insertMarkup(MarkupDTO dto);

	public abstract boolean markupExist(MarkupDTO dto);

	public abstract boolean isDuplicate(MarkupDTO dto);

}